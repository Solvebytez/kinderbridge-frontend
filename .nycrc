{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "check-coverage": true,
  "reporter": [
    "text",
    "text-summary",
    "html",
    "lcov"
  ],
  "exclude": [
    "coverage/**",
    "packages/*/test/**",
    "test/**",
    "test{,-*}.js",
    "**/*.test.js",
    "**/*.spec.js",
    "**/__tests__/**",
    "**/node_modules/**",
    "**/coverage/**",
    "**/.nyc_output/**",
    "**/dist/**",
    "**/build/**",
    "**/*.config.js",
    "**/*.config.ts"
  ],
  "branches": 80,
  "lines": 80,
  "functions": 80,
  "statements": 80
}
